class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int maxPro = 0;
        int n = prices.size();
        int mini = prices[0];
        for(int i = 0; i < n; i++){
            int pro = prices[i] - mini;
            maxPro = max(maxPro ,pro);
            mini = min(mini , prices[i]);
        }
        return maxPro;
   }
};
