class Solution {
public:
void f(TreeNode* root , vector<int>& ans){
    if(root == NULL) return;
    ans.push_back(root -> val);
    f(root -> left, ans);
    f(root -> right , ans);
}
    vector<int> preorderTraversal(TreeNode* root) {
        if(root != NULL) cout<<root->val;
        vector<int> ans;
        f(root , ans);
        return ans;
    }
};
